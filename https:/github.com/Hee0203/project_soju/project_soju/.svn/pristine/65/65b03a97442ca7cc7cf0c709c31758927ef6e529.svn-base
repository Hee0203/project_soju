*{
    font-family: 'Noto Sans KR', sans-serif;
    list-style: none;
    text-decoration: none;
    margin: 0 auto;
    padding: 0;
    color: black;
}
.contents{
    width: 100%;
    height: 800px;
    background-color: #f1f3f5;
}
.contents01{
    text-align: center;
    padding: 10px;
    background-color: white;
}

.contents02{
    width: 100%;
    height: 822px;
    text-align: center;
    background-color: #f1f3f5;
}
#div_span > span{
    display: block;
    width: 100%;
    height: 1px;
    background-color: #f1f3f5;
}

/* 테이블 영역 */
#div_table{
    width: 1000px;
    height: 400px;
}

/* List 영역 */
#div_table > table{
    width: 80%;
    height: 300px;
    padding-top: 40px;
}
th, td{
    text-align: center;
    border-bottom: 1px solid lightslategray;
    padding-top: 30px;
}
.left{
    font-size: 20px;
}
#update, #delete, #logout {
    background-color: rgb(142, 221, 182);
    border: none;
    border-radius: 3px;
    padding: 5px;
    margin-left: 10px;
}
.right > input{
    border: none;
    border-radius: 3px;
    height: 40px;
    width: 400px;
    font-size: 20px;
    background-color: #f1f3f5;
    text-align: center;
}